#!/bin/bash

# Get the directory of the script
SCRIPT_DIR=$(dirname "$(readlink -f "$0")")

# Navigate one levels up
ROOT_PATH=$(cd "." && pwd)

source "$SCRIPT_DIR/operations.sh"

main() {
	case $1 in
	set)
		set $2
		;;
	show)
		show
		;;
	remove)
		remove $2
		;;
	config)
		config $2
		;;
	*)
		echo "Unknown command: $1"
		echo "Usage: $0 {show|set|config|remove} [arguments...]"
		exit 1
		;;
	esac
}

main "$@"
